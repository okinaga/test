
public class Android {

	public static void main(String[] args){
		SmartPhone android=new SmartPhone();
		android.call();
		android.mail();
		android.photo();
		android.internet();
		android.play(){
			System.out.println("再生（ハイレゾ機能）");
		}
		android.stop();
		android.next();
		android.back();
	}
}
